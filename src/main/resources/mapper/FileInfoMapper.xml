<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhy.plateform.dao.FileInfoMapper">

  <resultMap id="BaseResultMap" type="com.jhy.plateform.domain.FileInfo" >
      <id column="id" property="id" jdbcType="INTEGER" />
      <result column="name" property="name" jdbcType="VARCHAR" />
      <result column="path" property="path" jdbcType="VARCHAR" />
      <result column="printCount" property="printCount" jdbcType="TINYINT" />
      <result column="page" property="page" jdbcType="TINYINT" />
      <result column="label" property="label" jdbcType="VARCHAR" />
      <result column="money" property="money" jdbcType="INTEGER" />
      <result column="createDate" property="createDate" jdbcType="TIMESTAMP" />
      <result column="modifyDate" property="modifyDate" jdbcType="TIMESTAMP" />
      <result column="userId" property="userId" jdbcType="INTEGER" />
  </resultMap>
  
  <sql id="Base_Column_List" >
      id,
      name,
      path,
      printCount,
      page,
      label,
      money,
      createDate,
      modifyDate,
      userId
  </sql>
  
  <!--根据Id查询-->
  <select id="findById" resultMap="BaseResultMap" parameterType="java.lang.String" >
      select <include refid="Base_Column_List" /> from fileInfo where id = #{id,jdbcType=INTEGER}
  </select>
  
  
  <!-- 多条件查询 -->
  <select id="findBySelective" resultMap="BaseResultMap" parameterType="com.jhy.plateform.query.FileInfoQuery">
      select <include refid="Base_Column_List" /> from fileInfo
      <trim prefix="where" prefixOverrides="and |or ">
         <if test="keyWords != null and keyWords != ''">
            <bind name="likeKeyWords" value="'%' + _parameter.keyWords + '%'" />
            and name like #{likeKeyWords,jdbcType=VARCHAR}
         </if>
      </trim>
      order by id DESC
  </select>
  
  
  <!-- 删除 -->
  <delete id="deleteById" parameterType="java.lang.String" >
      delete from fileInfo where id = #{id,jdbcType=INTEGER}
  </delete>
  
  <!-- 批量删除 -->
  <delete id="deleteByIds" parameterType="java.lang.String">
      delete from fileInfo where id in
      <foreach item="id" collection="array" open="(" separator="," close=")">
          #{id}
      </foreach>
  </delete>
  
  <!--部分字段允许为空-->
  <insert id="add" parameterType="com.jhy.plateform.domain.FileInfo" >
    insert into fileInfo
    <trim prefix="(" suffix=")" suffixOverrides="," >
        name,
        path,
        <if test="printCount != null" >
            printCount,
        </if>
        page,
        <if test="label != null and label != ''" >
            label,
        </if>
        money,
        createDate,
        modifyDate,
        userId
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
        #{name,jdbcType=VARCHAR},
        #{path,jdbcType=VARCHAR},
        <if test="printCount != null" >
            #{printCount,jdbcType=TINYINT},
        </if>
        #{page,jdbcType=TINYINT},
        <if test="label != null and label != ''" >
            #{label,jdbcType=VARCHAR},
        </if>
        #{money,jdbcType=INTEGER},
        #{createDate,jdbcType=TIMESTAMP},
        #{modifyDate,jdbcType=TIMESTAMP},
        #{userId,jdbcType=INTEGER}
    </trim>
  </insert>
  
  <!--更新--> 
  <update id="update" parameterType="com.jhy.plateform.domain.FileInfo" >
      update fileInfo
      <set>
          <if test="name != null and name != ''" >
              name = #{name,jdbcType=VARCHAR},
          </if>
          <if test="path != null and path != ''" >
              path = #{path,jdbcType=VARCHAR},
          </if>
          <if test="printCount != null" >
              printCount = #{printCount,jdbcType=TINYINT},
          </if>
          <if test="page != null" >
              page = #{page,jdbcType=TINYINT},
          </if>
          <if test="label != null and label != ''" >
              label = #{label,jdbcType=VARCHAR},
          </if>
          <if test="money != null" >
              money = #{money,jdbcType=INTEGER},
          </if>
          <if test="createDate != null" >
              createDate = #{createDate,jdbcType=TIMESTAMP},
          </if>
          <if test="modifyDate != null" >
              modifyDate = #{modifyDate,jdbcType=TIMESTAMP},
          </if>
          <if test="userId != null" >
              userId = #{userId,jdbcType=INTEGER}
          </if>
      </set>
      where id = #{id,jdbcType=INTEGER}
  </update>
	
</mapper>